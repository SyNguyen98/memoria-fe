{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{svg,png,jpg,gif}","*.{css,scss}","*.js"]
  },
  "globalHeaders": {
    "Access-Control-Allow-Origin": "https://chika-memoria.azurewebsites.net",
    "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
  }
}